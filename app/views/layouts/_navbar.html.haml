.header
  .container
    .logo 
      = link_to '/' do
        = image_tag "data:image/png;base64,", style: 'height:100px;width:100px;'
    .nav-left
      .ul.li.a
        = link_to "Clients", clients_path
      .ul.li.a
        = link_to "Plumbers", plumbers_path
      .ul.li.a
        = link_to "Events", events_path
    .nav-right
      .ul.li.b
        - if current_admin.nil?
          = link_to new_admin_session_path do 
            Login
            = image_tag "data:image/png;base64,", style: 'height:50px;width:50px;'
        - else
          = current_admin.name
          = link_to destroy_admin_session_path do 
            Logout
            = image_tag "data:image/png;base64,", style: 'height:20px;width:20px;'

:sass
  .container
    width: 2000px;
    margin: auto;

  .logo
    width: 100px;
    float: left;

  .header 
    background: white;

  .header:after 
    content: '';
    display: table;
    clear: both;

  .nav-left 
    float: left;
    margin-top: 30px;

  .nav-right
    float: right;
    margin-top: 30px;
    
  .ul 
    margin: 0;
    padding: 0;
    list-syle: none;
    padding-left: 20px;
    display: inline-block;
      
  .li 
    display: inline-block;

  .a 
    text-decoration: none;
    text-transform: uppercase;
    font-size: 24;
    display: inline-block;
    padding-left: 20px;

  .b 
    text-decoration: none;
    text-transform: uppercase;
    font-size: 16;
    display: inline-block;
    padding-left: 20px;
    padding-top: 5px;

  .a:hover 
    color: #000;



        
